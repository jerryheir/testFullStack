{
  "name": "stears-test",
  "version": "0.1.0",
  "description": "A Node.js application using typescript for the Stears Test",
  "main": "index.js",
  "author": "Jeremiah Nwaeze",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/server.js",
    "server": "nodemon src/server.ts",
    "build": "tsc -p .",
    "client": "yarn --cwd client/ start",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "test": "jest",
    "test:watch": "jest --watchAll --runInBand --detectOpenHandles"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/node": "14.0.5",
    "concurrently": "5.2.0",
    "nodemon": "2.0.4",
    "ts-node": "8.10.1",
    "typescript": "3.9.3"
  },
  "dependencies": {
    "@types/async": "3.2.3",
    "@types/cron": "1.7.2",
    "@types/jest": "25.2.3",
    "@types/mongoose": "5.7.21",
    "@types/morgan": "1.9.0",
    "@types/sinon": "9.0.4",
    "@types/supertest": "2.0.9",
    "async": "3.2.2",
    "cron": "1.8.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jest": "24.9.0",
    "mongoose": "5.9.15",
    "morgan": "1.10.0",
    "sinon": "9.0.2",
    "socket.io": "2.3.0",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0"
  }
}
